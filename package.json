{
  "name": "mujs-react",
  "version": "0.0.0",
  "description": "mujs-react",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "bin": {
    "mujs-react": "bundle/cli.js"
  },
  "files": [
    "dist",
    "bundle",
    "readme.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaoboy/mujs-react.git"
  },
  "homepage": "https://github.com/ahaoboy/mujs-react#readme",
  "keywords": [
    "mujs-react"
  ],
  "author": "ahaoboy",
  "license": "ISC",
  "scripts": {
    "esbuild:bundle": "esbuild ./src/* --bundle --outdir=esbuild --charset=utf8 --define:process.env.NODE_ENV='\"production\"' --minify",
    "esbuild:babel": "babel --presets  @babel/preset-env ./esbuild -d babel",
    "build:esbuild": "pnpm run esbuild:bundle && pnpm run esbuild:babel",
    "format": "deno fmt --ignore=node_modules,dist,bundle,target --indent-width=2 --no-semicolons --single-quote",
    "clear": "rm -rf node_modules dist bundle"
  },
  "dependencies": {
    "@mpv-easy/polyfill": "0.1.15-alpha.17"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "esbuild": "0.25.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}
