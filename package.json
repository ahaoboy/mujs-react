{
  "name": "mujs-react",
  "version": "0.0.0",
  "description": "mujs-react",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "bin": {
    "mujs-react": "bundle/cli.js"
  },
  "files": [
    "dist",
    "bundle",
    "readme.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaoboy/mujs-react.git"
  },
  "homepage": "https://github.com/ahaoboy/mujs-react#readme",
  "keywords": [
    "mujs-react"
  ],
  "author": "ahaoboy",
  "license": "ISC",
  "scripts": {
    "postinstall": "bun simple-git-hooks",
    "esbuild:bundle": "esbuild ./src/* --bundle --outdir=esbuild --charset=utf8 --define:process.env.NODE_ENV='\"production\"' --minify",
    "esbuild:babel": "babel --presets  @babel/preset-env ./esbuild -d babel",
    "build:esbuild": "pnpm run esbuild:bundle && pnpm run esbuild:babel",
    "build:vite": "vite build",
    "build:rsbuild": "rsbuild build",
    "build:rspack": "rspack build",
    "build:rollup": "rollup -c",
    "build:rolldown": "rolldown -c",
    "build:bunup": "bunup",
    "format": "deno fmt --ignore=node_modules,dist,bundle,target --indent-width=2 --no-semicolons --single-quote",
    "clear": "rm -rf node_modules dist bundle"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@mpv-easy/polyfill": "0.1.15-alpha.17",
    "@rollup/plugin-alias": "^5.1.1",
    "@rsbuild/core": "^1.5.17",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@vitejs/plugin-legacy": "^7.2.1",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "i": "^0.3.7",
    "rolldown": "1.0.0-beta.43",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "vite": "^7.1.10",
    "@types/bun": "^1.3.1",
    "bunup": "^0.15.12",
    "bumpp": "^10.3.1",
    "simple-git-hooks": "^2.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "esbuild": "0.25.10",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rollup": "^4.52.4",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}