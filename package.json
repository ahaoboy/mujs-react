{
  "name": "mujs-react",
  "version": "0.0.0",
  "description": "mujs-react",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "bin": {
    "mujs-react": "bundle/cli.js"
  },
  "files": [
    "dist",
    "bundle",
    "readme.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaoboy/mujs-react.git"
  },
  "homepage": "https://github.com/ahaoboy/mujs-react#readme",
  "keywords": [
    "mujs-react"
  ],
  "author": "ahaoboy",
  "license": "ISC",
  "scripts": {
    "postinstall": "bun simple-git-hooks",
    "esbuild:bundle": "esbuild ./src/* --bundle --outdir=esbuild --charset=utf8 --define:process.env.NODE_ENV='\"production\"' --minify",
    "esbuild:bundle-tsc": "esbuild ./tsc/* --bundle --outdir=esbuild --charset=utf8 --define:process.env.NODE_ENV='\"production\"' --minify",
    "esbuild:babel": "babel --presets  @babel/preset-env ./esbuild -d babel",
    "build:esbuild": "pnpm run esbuild:bundle && pnpm run esbuild:babel",
    "build:vite": "vite build",
    "build:rsbuild": "rsbuild build",
    "build:rspack": "rspack build",
    "build:rollup": "rollup -c",
    "build:rolldown": "rolldown -c",
    "build:bunup": "bunup",
    "build:farm": "farm build",
    "build:tsc": "tsc -p tsconfig.es5.json",
    "build:swc": "swc ./esbuild/index.js -o ./swc/index.js --sync",
    "build:parcel": "parcel build src/index.tsx --dist-dir parcel --no-source-maps",
    "build:swcpack": "spack ./tsc/index.js -o ./swc/index.js",
    "build:webpack": "webpack --config webpack.config.js",
    "format": "deno fmt --ignore=node_modules,dist,bundle,target --indent-width=2 --no-semicolons --single-quote",
    "clear": "rm -rf node_modules dist bundle"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.27.1",
    "@babel/preset-react": "7.28.5",
    "@farmfe/cli": "^1.0.5",
    "@farmfe/core": "^1.7.11",
    "@mpv-easy/polyfill": "0.1.15-alpha.17",
    "@parcel/config-default": "^2.16.1",
    "@parcel/packager-ts": "^2.16.1",
    "@rollup/plugin-alias": "6.0.0",
    "@rsbuild/core": "1.6.6",
    "@rsbuild/plugin-react": "1.4.2",
    "@rspack/cli": "1.6.3",
    "@rspack/core": "1.6.3",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.2",
    "@types/bun": "1.3.2",
    "@vitejs/plugin-legacy": "7.2.1",
    "babel-loader": "10.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "buffer": "6.0.3",
    "bumpp": "10.3.1",
    "bunup": "0.15.14",
    "esbuild-plugin-babel": "^0.2.3",
    "parcel": "^2.16.1",
    "quick-lru": "^7.3.0",
    "rolldown": "1.0.0-beta.50",
    "rollup-plugin-polyfill-node": "0.13.0",
    "vite": "7.2.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@parcel/transformer-babel": "^2.16.1",
    "@parcel/transformer-typescript-tsc": "^2.16.1",
    "@parcel/transformer-typescript-types": "2.16.1",
    "@rollup/plugin-babel": "6.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-replace": "6.0.3",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "esbuild": "0.27.0",
    "rollup": "4.53.2",
    "simple-git-hooks": "2.13.1",
    "typescript": "5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "echo pre-commit"
  },
  "pnpm": {
    "neverBuiltDependencies": []
  }
}